    "bulk-mint:json": "hardhat run scripts/bulk-mint-from-json.ts",
    "bulk-mint:json:amoy": "hardhat run scripts/bulk-mint-from-json.ts --network amoy",
    "bulk-mint:db": "hardhat run scripts/bulk-mint-from-database.ts",
    "bulk-mint:db:amoy": "hardhat run scripts/bulk-mint-from-database.ts --network amoy",
