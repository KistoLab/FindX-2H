    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "graphql": "^16.11.0",
    "livekit-server-sdk": "^2.13.3",
    "dotenv": "^17.0.0",
    "tsconfig-paths": "^4.2.0",
    "mongoose": "^8.18.1",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "eslint": "^9",
