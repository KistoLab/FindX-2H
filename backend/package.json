{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx -r tsconfig-paths/register src/server.ts",
    "predev": "graphql-codegen --config codegen.yml && node scripts/fix-enum-order.js",
    "build": "graphql-codegen --config codegen.yml && node scripts/fix-enum-order.js && next build --no-lint",
    "start": "tsx -r tsconfig-paths/register src/server.ts",
    "start:prod": "next start",
    "lint": "eslint",
    "codegen": "graphql-codegen --config codegen.yml && node scripts/fix-enum-order.js"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "graphql": "^16.11.0",
    "livekit-server-sdk": "^2.13.3",
    "dotenv": "^17.0.0",
    "tsconfig-paths": "^4.2.0",
    "mongoose": "^8.18.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
